<%= form_for [:admin, @order] do |f| %>
<div id="completed">
   <%= f.radio_button(:status, "completed") %>
   <%= f.label(:status, "Complete") %>
 </div>
 <div id="cancelled">
   <%= f.radio_button(:status, "cancelled") %>
   <%= f.label(:status, "Cancel") %>
 </div>
   <%= f.submit "Submit" %>
 <% end %>
